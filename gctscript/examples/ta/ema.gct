fmt := import("fmt")
exch := import("exchange")
t := import("times")
sma := import("indicator/sma")
ema := import("indicator/ema")

load := func() {
    start := t.add(t.now(), -t.hour*24*100)
    ohlcvData := exch.ohlcv("BTC Markets", "BTC-AUD", "-", "", start, t.now(), "1d")

    ret := ema.calculate(ohlcvData.candles, 100)
    fmt.println(ret)

    ret = sma.calculate(ohlcvData.candles, 100)
    fmt.println(ret)
}

load()
