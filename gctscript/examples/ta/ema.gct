fmt := import("fmt")
exch := import("exchange")
t := import("times")
ind := import("indicator/moving-average")

load := func() {
    start := t.add(t.now(), -t.hour*24*100)
    ohlcvData := exch.ohlcv("BTC Markets", "BTC-AUD", "-", "", start, t.now(), t.hour*24)

    ret := ind.ema(ohlcvData.candles, 100)
    fmt.println(ret)

    ret = ind.sma(ohlcvData.candles, 100)
    // fmt.println(ret)
}

load()
