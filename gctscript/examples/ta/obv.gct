fmt := import("fmt")
exch := import("exchange")
t := import("times")
obv := import("indicator/obv")

load := func() {
    start := t.add(t.now(), -t.hour*24*100)
    ohlcvData := exch.ohlcv("BTC Markets", "BTC-AUD", "-", "", start, t.now(), "1d")

    ret := obv.calculate("close", ohlcvData.candles)
    fmt.println(ret)
}

load()
