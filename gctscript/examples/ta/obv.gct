fmt := import("fmt")
exch := import("exchange")
t := import("times")
obv := import("indicator/obv")

load := func() {
    start := t.add(t.now(), -t.hour*24)
    ohlcvData := exch.ohlcv("coinbasepro", "BTC-USD", "-", "SPOT", start, t.now(), "1h")

    ret := obv.calculate(ohlcvData.candles)
    fmt.println(ret)
}

load()

