fmt := import("fmt")
exch := import("exchange")
t := import("times")
obv := import("indicator/obv")

load := func() {
    start := t.date(2019, 1 , 1 , 0 , 0 , 0, 0)
    end := t.add_date(start, 0, 6 , 0)
    ohlcvData := exch.ohlcv("coinbasepro", "BTC-USD", "-", "SPOT", start, end, "1d")

    ret := obv.calculate(ohlcvData.candles)
    fmt.println(ret)
}

load()

