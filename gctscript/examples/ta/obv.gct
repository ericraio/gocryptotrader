fmt := import("fmt")
exch := import("exchange")
t := import("times")
ind := import("indicator/volume")

load := func() {
    start := t.add(t.now(), -t.hour*24*100)
    ohlcvData := exch.ohlcv("BTC Markets", "BTC-AUD", "-", "", start, t.now(), t.hour*24)

    ret := ind.obv("close", ohlcvData.candles)
    fmt.println(ret)
}

load()
