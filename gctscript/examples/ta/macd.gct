fmt := import("fmt")
exch := import("exchange")
t := import("times")
macd := import("indicator/macd")

load := func() {
    start := t.add(t.now(), -t.hour*100)
    ohlcvData := exch.ohlcv("coinbasepro", "BTC-USD", "-", "SPOT", start, t.now(), "1h")

    ret := macd.calculate(ohlcvData.candles, 12, 26, 9)
    fmt.println(ret)
}

load()
