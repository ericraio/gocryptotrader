fmt := import("fmt")
exch := import("exchange")
t := import("times")
macd := import("indicator/macd")

load := func() {
    start := t.add(t.now(), -t.hour*24*7)
    ohlcvData := exch.ohlcv("BTC Markets", "BTC-AUD", "-", "SPOT", start, t.now(), "1h")
    ret := macd.calculate(ohlcvData.candles, 12, 26, 9)
    fmt.println(ret)
}

load()
