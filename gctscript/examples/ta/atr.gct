fmt := import("fmt")
exch := import("exchange")
t := import("times")
atr := import("indicator/atr")

load := func() {
    start := t.add(t.now(), -t.hour*24)
    ohlcvData := exch.ohlcv("coinbasepro", "BTC-USD", "-", "SPOT", start, t.now(), "1h")

    ret := atr.calculate(ohlcvData.candles, 14)
    fmt.println(ret)
}

load()
